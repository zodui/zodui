{
  "name": "@zodui/core",
  "version": "0.2.0",
  "license": "MIT",
  "main": "dist/esm/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/dts/index.d.ts",
  "typesVersions": {
    "<5.0": { "*": [ "*", "dist/dts/*", "dist/dts/*/index.d.ts" ] }
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "inner-src": "./src/index.ts",
      "import": {
        "types": "./dist/dts/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/dts/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    },
    "./utils": {
      "inner-src": "./src/utils.ts",
      "import": {
        "types": "./dist/dts/utils.d.ts",
        "default": "./dist/esm/utils.js"
      },
      "require": {
        "types": "./dist/dts/utils.d.ts",
        "default": "./dist/cjs/utils.js"
      }
    },
    "./external": {
      "inner-src": "./src/external.ts",
      "import": {
        "types": "./dist/dts/external.d.ts",
        "default": "./dist/esm/external.js"
      },
      "require": {
        "types": "./dist/dts/external.d.ts",
        "default": "./dist/cjs/external.js"
      }
    }
  },
  "files": [
    "dist/cjs",
    "dist/esm",
    "dist/dts",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rollup -c && yarn build:dts",
    "build:dts": "tsc -p tsconfig.dts.json --outDir dist/dts"
  },
  "dependencies": {
    "zod": "^3.21.4"
  }
}
